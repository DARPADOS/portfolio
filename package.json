{
  "name": "jlallaucca-portfolio",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npx astro-i18next generate && astro dev",
    "start": "npx astro-i18next generate && astro dev",
    "build": "npx astro-i18next generate && astro build",
    "preview": "npm run build && wrangler pages dev dist",
