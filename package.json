    "test-prod": "npm run build && npx servor dist index.html 3000 --reload --browse --static",
    "publish": "wrangler pages publish dist"
  },
  "devDependencies": {
    "astro": "^1.9.1"
  },
  "dependencies": {
    "astro-i18next": "1.0.0-beta.15",
    "wrangler": "^2.9.0"
  }
}
