---
import GithubIcon from '@icons/GithubIcon.astro';
import JavaIcon from '@icons/JavaIcon.astro';
import NextjsIcon from '@icons/NextjsIcon.astro';
import PostgreSQLIcon from '@icons/PostgreSQLIcon.astro';
import PrismaIcon from '@icons/PrismaIcon.astro';
import ReactIcon from '@icons/ReactIcon.astro';
import SolanaIcon from '@icons/SolanaIcon.astro';
import SpringBootIcon from '@icons/SpringBootIcon.astro';
import TailwindIcon from '@icons/TailwindIcon.astro';
import ThymeleafIcon from '@icons/ThymeleafIcon.astro';
import TypescriptIcon from '@icons/TypescriptIcon.astro';

export interface Props {
  icon: string
}

const {icon} = Astro.props
---
{
  (() => {
    switch (icon) {
      case "nextjs":
        return <NextjsIcon />
      case "react":
        return <ReactIcon />
      case "tailwind":
        return <TailwindIcon />
      case "prisma":
        return <PrismaIcon />
      case "typescript":
        return <TypescriptIcon />
      case "postgres":
        return <PostgreSQLIcon />
      case "solana":
        return <SolanaIcon />
      case "thymeleaf":
        return <ThymeleafIcon />
      case "java":
        return <JavaIcon />
      case "spring-boot":
        return <SpringBootIcon />
      default:
        break;
    }
  })()
}
